@model List<AmazonWeb.Models.Order>

<h2>Order History</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>User ID</th>
            <th>Order Date</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderID</td>
                <td>@order.UserID</td>
                <td>@order.OrderDate.ToShortDateString()</td>
                <td>@order.Status</td>
            </tr>
        }
    </tbody>
</table>
